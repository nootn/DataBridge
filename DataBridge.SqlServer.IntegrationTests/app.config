<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
    <section name="SqlServerSourceConfigSection" type="DataBridge.SqlServer.Configuration.SqlServerSourceConfigSection, DataBridge.SqlServer" allowLocation="true" allowDefinition="Everywhere" />
    <section name="ThrowsNoTableError" type="DataBridge.SqlServer.Configuration.SqlServerSourceConfigSection, DataBridge.SqlServer" allowLocation="true" allowDefinition="Everywhere" />
  </configSections>
  <appSettings>
    <add key="SeqServerUrl" value="http://localhost:5341" />
  </appSettings>
  <SqlServerSourceConfigSection>
    <SourceDatabaseConnectionString value="Data Source=(localdb)\MSSQLLocalDB;Initial Catalog=DataBridge.SqlServer.IntegrationTests.Source;Integrated Security=True;" />
    <SourceTables>
      <!-- changeDetectionMode: 1 = ASAP, 2 = Polling -->
      <add id="dbo.Test" name="Test" schema="dbo" changeDetectionMode="2" pollIntervalInMilliseconds="5000" qualityCheckIntervalInMilliseconds="60000" qualityCheckRecordBatchSize="10" />
    </SourceTables>
  </SqlServerSourceConfigSection>
  <ThrowsNoTableError>
    <SourceDatabaseConnectionString value="Data Source=(localdb)\MSSQLLocalDB;Initial Catalog=DataBridge.SqlServer.IntegrationTests.Source;Integrated Security=True;" />
    <SourceTables>
    </SourceTables>
  </ThrowsNoTableError>
  <runtime>
    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
      <dependentAssembly>
        <assemblyIdentity name="nunit.framework" publicKeyToken="2638cd05610744eb" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-3.5.0.0" newVersion="3.5.0.0" />
      </dependentAssembly>
    </assemblyBinding>
  </runtime>
</configuration>